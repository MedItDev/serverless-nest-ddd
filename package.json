{
  "name": "serverless-nest-ddd",
  "version": "1.0.0",
  "description": "Serverless Nest DDD Boilerplate",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "build": "nest build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MedItDev/serverless-nest-ddd.git"
  },
  "keywords": [
    "Serverless",
    "Nest",
    "DDD",
    "AWS",
    "Lambda"
  ],
  "author": "andrzej.konarski@medit.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MedItDev/serverless-nest-ddd/issues"
  },
  "homepage": "https://github.com/MedItDev/serverless-nest-ddd#readme",
  "devDependencies": {
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/aws-lambda": "^8.10.102",
    "@types/node": "^18.7.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.35.2-alpha.3",
    "@typescript-eslint/parser": "5.35.2-alpha.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-security": "1.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^9.1.7",
    "serverless-plugin-typescript": "^2.1.2",
    "serverless-plugin-warmup": "^7.3.0",
    "supertest": "^6.2.4",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
